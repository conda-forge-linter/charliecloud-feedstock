+  ENABLE_CH_IMAGE_TRUE='#'
+  ENABLE_CH_IMAGE_FALSE=
 else
   ENABLE_CH_IMAGE_TRUE='#'
   ENABLE_CH_IMAGE_FALSE=
@@ -7425,8 +7425,8 @@ fi
   ENABLE_TEST_TRUE=
   ENABLE_TEST_FALSE='#'
 else
-  ENABLE_TEST_TRUE='#'
-  ENABLE_TEST_FALSE=
